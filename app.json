{
  "expo": {
    "name": "StatLocker",
    "slug": "statlocker-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/statlocker-appicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F7F9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.statlocker.app",
      "backgroundColor": "#F7F7F9"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F7F7F9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.statlocker.app",
      "backgroundColor": "#F7F7F9"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#F7F7F9"
    },
    "extra": {
      "eas": {
        "projectId": "statlocker-react-native"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
